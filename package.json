{
  "name": "eng-test",
  "version": "1.0.0",
  "description": "Multi-user English Test System",
  "scripts": {
    "install:all": "npm install && cd server && npm install && cd ../client && npm install",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm run dev",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "prod:backend": "cd server && NODE_ENV=production node index.js",
    "prod:frontend": "cd client && npx serve -s build -l 3000"
  },
  "keywords": ["english", "test", "quiz", "education"],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
